[  {    "description": "<p>Request diagnosis information and all codes related to that request. For example, if you know the SNOMED code and want to find the related ICD9 or ICD10 code, you can use the API to request that information.</p>",    "name": "GetDiagnosis",    "group": "Diagnosis_Lookup",    "examples": [      {        "title": "Example usage:",        "content": "curl -i http://localhost:3000/api/v1/lookup/diagnosis/V57.89",        "type": "curl"      }    ],    "type": "get",    "url": "/api/v1/lookup/diagnosis/:id",    "title": "",    "version": "0.1.1",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "string",            "optional": false,            "field": "query",            "description": "<p>string for diagnosis code (optional), if nothing entered, return all.</p>"          }        ]      }    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "snomed",            "description": "<p>SNOMED code.</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "snomed_name",            "description": "<p>SNOMED description / name.</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "icd10_code",            "description": "<p>ICD10 code.</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "icd10_name",            "description": "<p>ICD10 description / name.</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "icd9_code",            "description": "<p>ICD9 code.</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "icd9_name",            "description": "<p>ICD9 description / name.</p>"          }        ]      },      "examples": [        {          "title": "Success-Response:",          "content": "HTTP/1.1 200 OK\n{\n  snomed: \"305839006\",\n  snomed_name: \"Seen by arts therapy service (finding)\",\n  icd10_code: \"Z76.89\",\n  icd10_name: \"Persons encountering health services in other specified circumstances\",\n  icd9_code: \"V57.89\",\n  icd9_name: \"Care involving other specified rehabilitation procedure\"\n}",          "type": "json"        }      ]    },    "sampleRequest": [      {        "url": "http://localhost:3000/api/v1/lookup/rx/"      }    ],    "filename": "models/diagnosis.js",    "groupTitle": "Diagnosis_Lookup"  },  {    "description": "<p>Request rx information and all codes related to that request. For example, if you know the RX Norm code and want to find the related NCD code, you can use the API to request that information.</p>",    "name": "GetRx",    "group": "Rx_Lookup",    "examples": [      {        "title": "Example usage:",        "content": "curl -i http://localhost:3000/api/v1/lookup/rx/577095",        "type": "curl"      }    ],    "type": "get",    "url": "api/v1/lookup/rx/:id",    "title": "",    "version": "0.1.0",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "string",            "optional": false,            "field": "search",            "description": "<p>string for rx code (optional), if nothing entered, return all.</p>"          }        ]      }    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "id",            "description": "<p>Unique SocialCare ID (may change over time).</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "rx",            "description": "<p>code for the requested query.</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "rx_code",            "description": "<p>The RX Norm Code for the requested query.</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "rx_type",            "description": "<p>The RX type (ex: clinical) for the requested query.</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "rx_brand",            "description": "<p>RX brand for for the requested query.</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "ndc",            "description": "<p>NDC code for the requested query.</p>"          }        ]      },      "examples": [        {          "title": "Success-Response:",          "content": "HTTP/1.1 200 OK\n{\n  id: 1,\n  rx: \"6809\",\n  rx_code: \"577095\",\n  rx_type: \"Clinical\",\n  rx_brand: \"Metformin 850 MG / pioglitazone 15 MG Oral Tablet\",\n  ndc: \"11532002201\"\n}",          "type": "json"        }      ]    },    "sampleRequest": [      {        "url": "http://localhost:3000/api/v1/lookup/rx/"      }    ],    "filename": "models/lookup_rx.js",    "groupTitle": "Rx_Lookup"  }]
